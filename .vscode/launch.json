{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "name": "x-monorepo web-nuxt dev",
      "request": "launch",
      "url": "http://localhost:2011",
      "env": {
        "NODE_ENV": "development"
      },
      "sourceMaps": true,
      "webRoot": "${workspaceFolder}/apps/web-nuxt"
    },
    {
      "type": "chrome",
      "name": "x-monorepo admin ele dev",
      "request": "launch",
